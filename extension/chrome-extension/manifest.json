{
  "manifest_version": 3,
  "name": "screen/ screenshot tool",
  "short_name": "screen/",
  "description": "Take screenshots and upload to hosted screen/ instance.",
  "homepage_url": "https://example.com,",
  "version": "1.0",

  "action": {
    "tooltip": "Take A Screenshot"
  },
  "icons": {
    "128": "images/icon-screenshot-512.png",
    "512": "images/icon-screenshot-512.png"
  },
  "background": {
    "service_worker": "js/sw.js"
  },
  "permissions": ["activeTab"]
}
